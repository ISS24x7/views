{
    "version": 0.2,
    "phases": {
        "install": {
            "commands": [
                "npm install -g webpack"
            ]
        },
        "pre_build": {
            "commands": []
        },
        "build": {
            "commands": [
                "npm run build"
            ]
        },
        "post_build": {
            "commands": []
        }
    },
    "artifacts": {
        "files": [
            "**/*"
        ],
        "discard-paths": "yes",
        "base-directory": "dist"
    }
}
